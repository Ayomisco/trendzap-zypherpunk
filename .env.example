# TrendZap Environment Configuration
# Copy this file to .env and fill in your values

# === Solana ===
SOLANA_NETWORK=devnet
HELIUS_RPC_URL=https://devnet.helius-rpc.com/?api-key=YOUR_HELIUS_API_KEY
SOLANA_PROGRAM_ID=

# === Arbitrum ===
ARBITRUM_RPC_URL=https://sepolia-rollup.arbitrum.io/rpc
ARBITRUM_PRIVATE_KEY=
ORCHESTRATOR_ADDRESS=
ARBISCAN_API_KEY=

# === Axelar ===
AXELAR_ENV=testnet
AXELAR_RPC_URL=https://rpc-axelar-testnet.imperator.co:443

# === NEAR (for intents) ===
NEAR_NETWORK=testnet
NEAR_ACCOUNT_ID=
NEAR_PRIVATE_KEY=

# === Oracle ===
TWITTER_BEARER_TOKEN=
ORACLE_PRIVATE_KEY=

# === Zcash (optional for MVP) ===
ZCASH_RPC_URL=https://testnet.zcash.com
ZCASH_RPC_USER=
ZCASH_RPC_PASSWORD=

# === Frontend ===
NEXT_PUBLIC_HELIUS_RPC_URL=${HELIUS_RPC_URL}
NEXT_PUBLIC_ARBITRUM_RPC_URL=${ARBITRUM_RPC_URL}
NEXT_PUBLIC_SOLANA_PROGRAM_ID=${SOLANA_PROGRAM_ID}
NEXT_PUBLIC_ORCHESTRATOR_ADDRESS=${ORCHESTRATOR_ADDRESS}
NEXT_PUBLIC_PRIVY_APP_ID=
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=

# === Development ===
NODE_ENV=development
LOG_LEVEL=debug
